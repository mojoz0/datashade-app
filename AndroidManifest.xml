<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.datashade"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.datashade.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.datashade.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.example.datashade" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.datashade" />
        </activity>
        <activity
            android:name="com.example.datashade.DataShadeGrid"
            android:label="@string/title_activity_data_shade_grid"
            android:parentActivityName="com.example.datashade.GoalList" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.datashade" />
        </activity>
        <activity
            android:name="com.example.datashade.GoalList"
            android:label="@string/title_activity_goal_list"
            android:parentActivityName="com.example.datashade.StudentList" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.datashade.MainActivity" />
        </activity>
        <activity
            android:name="com.example.datashade.StudentList"
            android:label="@string/title_activity_student_list"
            android:parentActivityName="com.example.datashade.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.datashade.MainActivity" />
        </activity>
    </application>

</manifest>
